{{#each posts}}
<form action="/editarPost/{{dataValues.id}}" method="POST">
    <h1>Editar Postagens</h1>
    <p>Titulo:</p><input type="text" name="titulo" value="{{dataValues.titulo}}">
    <p>Conteudo</p> <textarea name="conteudo" rows="10" cols="30" >{{dataValues.conteudo}}</textarea>
    <p><button type="submit">Editar postagens</button></p>
</form>
{{/each}}